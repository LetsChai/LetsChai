# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Soft Beta Launch
GET     /                           controllers.Application.landing()
GET     /login                      controllers.Login.fbLoginRedirect()
GET     /login/code/                controllers.Login.extractCode(code)
GET     /testuser/create            controllers.Login.createTestUser()
GET     /testuser/login             controllers.Login.loginTestUser()
GET     /landing                    controllers.Application.landing()
GET     /da39a3ee5e6b4b0d3255bfef95601890afd80709                   controllers.Application.thankyou()
GET     /newusersurvey              controllers.Application.newusersurvey()
GET     /chai                       controllers.Application.chai()
GET     /editprofile                controllers.Application.editprofile()

# Full Launch



# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)



